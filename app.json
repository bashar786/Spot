{
  "expo": {
    "name": "Spot",
    "slug": "Spot",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/spotnew.jpeg",  
    "splash": {
      "image": "./assets/images/spotnew.jpeg",  
      "backgroundColor": "#1E3B2F",
      "resizeMode": "contain",
      "imageResizeMode": "contain",
      "android": {
        "backgroundColor": "#1E3B2F",
        "resizeMode": "cover",
        "xxhdpi": "./assets/images/spotnew.jpeg",  
        "xxxhdpi": "./assets/images/spotnew.jpeg"  
      },
      "ios": {
        "backgroundColor": "#1E3B2F",
        "resizeMode": "contain",
        "xxhdpi": "./assets/images/spotnew.jpeg",  
        "xxxhdpi": "./assets/images/spotnew.jpeg"  
      }
    },
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "scheme": "spot",
      "bundleIdentifier": "com.haider434.Spot"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/spotnew.jpeg",  
        "backgroundColor": "#1E3B2F"
      },
      "scheme": "spot",
      "package": "com.haider434.Spot"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5f5441a2-67d8-4327-820c-90f674e2bad6"
      }
    }
  }
}
